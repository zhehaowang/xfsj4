function Trig_FilmActions takes nothing returns nothing
	call CinematicFadeBJ(bj_CINEFADETYPE_FADEOUT,0.00,"ReplaceableTextures\\CameraMasks\\White_mask.blp",0,0,0,0)
	call DisplayTimedTextToForce(GetPlayersAll(),5.00,"感谢西方4讨论设计科的鼎力支持")
	call TriggerSleepAction(5.00)
	if((GetPlayerController(Player(0))==MAP_CONTROL_USER)and(GetPlayerSlotState(Player(0))==PLAYER_SLOT_STATE_PLAYING))then
		call DialogSetMessage(udg_dialog01,"请选择游戏难度")
		call DialogAddButtonBJ(udg_dialog01,"|cffffcc33简单(新手入门)|r")
		set udg_buttons01[1]=GetLastCreatedButtonBJ()
		call DialogAddButtonBJ(udg_dialog01,"|cffcc3300困难(高手推荐)|r")
		set udg_buttons01[2]=GetLastCreatedButtonBJ()
		call DialogAddButtonBJ(udg_dialog01,"|cff9900ff噩梦(建议组队)|r")
		set udg_buttons01[3]=GetLastCreatedButtonBJ()
		call DialogDisplayBJ(true,udg_dialog01,Player(0))
		call DestroyTrigger(GetTriggeringTrigger())
	else
	endif
	call SetTimeOfDay(12)
endfunction

function Trig_InzActions takes nothing returns nothing
