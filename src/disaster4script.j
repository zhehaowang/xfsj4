function MeatHook__Cycle takes nothing returns nothing
	local string key=I2S((GetHandleId((GetExpiredTimer()))))
	local integer total=(LoadInteger(udg_hashtable01,StringHash((key)),StringHash(("ChainLinkNumber"))))
	local integer size=(LoadInteger(udg_hashtable01,StringHash((key)),StringHash(("ChainLinkNumberMax"))))
	local integer utid=(LoadInteger(udg_hashtable01,StringHash((key)),StringHash(("ChainLinkId"))))
	local integer switch=(LoadInteger(udg_hashtable01,StringHash((key)),StringHash(("Switch"))))
	local unit pudge=(LoadUnitHandle(udg_hashtable01,StringHash((key)),StringHash(("Pudge"))))
	local unit target=(LoadUnitHandle(udg_hashtable01,StringHash((key)),StringHash(("Target"))))
	local real dist=(LoadReal(udg_hashtable01,StringHash((key)),StringHash(("Distance"))))
	local real l
	local real f
	local real x
	local real y
	local real xvel
	local real yvel
	local unit array part
	local integer i=0
	loop
		exitwhen i==total
		set part[i]=(LoadUnitHandle(udg_hashtable01,StringHash((key)),StringHash(("ChainLinks"+I2S(i)))))
		set i=i+1
	endloop
	if target==null then
		call MeatHook__HookAim(key,pudge,GetUnitX(part[0]),GetUnitY(part[0]),MeatHook__GetUnitZ(part[0]))
	else
		call SetUnitX(target,GetUnitX(part[0]))
		call SetUnitY(target,GetUnitY(part[0]))
		if GetLocalPlayer()==GetOwningPlayer(target)then
			call SelectUnit(target,false)
		endif
	endif
	if switch==1 then
		set x=GetUnitX(part[0])+(LoadReal(udg_hashtable01,StringHash((key)),StringHash(("VelocityX"))))
		set y=GetUnitY(part[0])+(LoadReal(udg_hashtable01,StringHash((key)),StringHash(("VelocityY"))))
		call SetUnitX(part[0],x)
		call SetUnitY(part[0],y)
		if(RMinBJ(RMaxBJ(((x)*1.0),udg_real02),udg_real01))!=x or(RMinBJ(RMaxBJ(((y)*1.0),udg_real04),udg_real03))!=y then
			call SaveInteger(udg_hashtable01,StringHash((key)),StringHash(("Switch")),(2))
		endif
		set i=1
		loop
			exitwhen i==total
			set f=Atan2(GetUnitY(part[i-1])-GetUnitY(part[i]),GetUnitX(part[i-1])-GetUnitX(part[i]))
			call SetUnitX(part[i],GetUnitX(part[i])+Cos(f)*40.0)
			call SetUnitY(part[i],GetUnitY(part[i])+Sin(f)*40.0)
			call SetUnitFacing(part[i],f*bj_RADTODEG)
			set i=i+1
		endloop
	elseif switch==2 then
		set l=(50.0+40.0)*(50.0+40.0)
		set i=total-1
		loop
			exitwhen i<0
			if part[i+1]==null then
				set xvel=GetUnitX(part[i])-GetUnitX(pudge)
				set yvel=GetUnitY(part[i])-GetUnitY(pudge)
				if xvel*xvel+yvel*yvel<=l then
					call RemoveUnit(part[i])
					set part[i]=null
					set total=total-1
					call SaveInteger(udg_hashtable01,StringHash((key)),StringHash(("ChainLinkNumber")),(total))
				endif
			else
				set xvel=GetUnitX(part[i])-GetUnitX(part[i+1])
				set yvel=GetUnitY(part[i])-GetUnitY(part[i+1])
			endif
			if part[i]!=null then
				set f=Atan2(yvel,xvel)
				call SetUnitX(part[i],GetUnitX(part[i])-Cos(f)*40.0)
				call SetUnitY(part[i],GetUnitY(part[i])-Sin(f)*40.0)
				call SetUnitFacing(part[i],f*bj_RADTODEG)
			endif
			set i=i-1
		endloop
		if total<1 then
			call SaveInteger(udg_hashtable01,StringHash((key)),StringHash(("Switch")),(0))
			set switch=0
		endif
	endif
	if switch>0 then
		set x=GetUnitX(part[total-1])
		set y=GetUnitY(part[total-1])
		set xvel=x-GetUnitX(pudge)
		set yvel=y-GetUnitY(pudge)
		set l=SquareRoot(xvel*xvel+yvel*yvel)
		if l>dist+50.0 then
			set f=bj_RADTODEG*Atan2(yvel,xvel)
			set xvel=xvel*(dist/l)
			set yvel=yvel*(dist/l)
			loop
				exitwhen l<dist
				if total>size then
					call SaveInteger(udg_hashtable01,StringHash((key)),StringHash(("Switch")),(2))
					exitwhen true
				endif
				set x=x-xvel
				set y=y-yvel
				set part[total]=CreateUnit(GetOwningPlayer(pudge),utid,x,y,f)
				call SaveUnitHandle(udg_hashtable01,StringHash((key)),StringHash(("ChainLinks"+I2S(total))),(part[total]))
				set total=total+1
				set l=l-dist
			endloop
			call SaveInteger(udg_hashtable01,StringHash((key)),StringHash(("ChainLinkNumber")),(total))
		endif
		if l>50.0+40.0*2 then
			set l=(50.0+40.0)/dist
			set xvel=xvel*l
			set yvel=yvel*l
			call SetUnitX(part[total-1],GetUnitX(pudge)+xvel)
			call SetUnitY(part[total-1],GetUnitY(pudge)+yvel)
			call SetUnitFacing(part[total-1],Atan2(yvel,xvel)*bj_RADTODEG)
			set i=total-2
			loop
				exitwhen i<0
				set xvel=GetUnitX(part[i])-GetUnitX(part[i+1])
				set yvel=GetUnitY(part[i])-GetUnitY(part[i+1])
				set l=dist/SquareRoot(xvel*xvel+yvel*yvel)
				call SetUnitX(part[i],GetUnitX(part[i+1])+xvel*l)
				call SetUnitY(part[i],GetUnitY(part[i+1])+yvel*l)
				call SetUnitFacing(part[i],Atan2(yvel,xvel)*bj_RADTODEG)
				set i=i-1
			endloop
		endif
	endif
	if switch==0 then
		call YDWESyStemAbilityCastingOverTriggerAction(pudge,5)
		call FlushChildHashtable(udg_hashtable01,StringHash((key)))
		call GroupRemoveUnit(udg_group02,target)
		call PauseTimer(GetExpiredTimer())
		call DestroyTimer(GetExpiredTimer())
	endif
	set i=0
	loop
		exitwhen i>=total
		set part[i]=null
		set i=i+1
	endloop
	set target=null
	set pudge=null
	set key=""
endfunction

